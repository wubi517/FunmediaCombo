            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.VideoIjkPlayActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.SeriesExoPlayActivity"
            android:screenOrientation="landscape"
