        <activity
            android:name=".activity.PreviewSeriesActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.SeriesCatActivity"
            android:screenOrientation="landscape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.SeriesPlayActivity"
            android:screenOrientation="landscape"
